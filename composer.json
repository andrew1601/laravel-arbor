{
    "name": "andrew1601/laravel-arbor",
    "description": "A minimal package to quickly use the Arbor Education API in a Laravel project",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Pope",
            "email": "andrew@andrew-pope.co.uk"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "require": {
        "softonic/graphql-client": "^3.0"
    },
    "autoload": {
      "psr-4": {
        "ANP\\LaravelArbor\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "ANP\\LaravelArbor\\ArborGraphQLServiceProvider"
        ],
        "aliases": {
          "ArborGraphQL": "ANP\\LaravelArbor\\Facades\\ArborGraphQL"
        }
      }
    }
}
